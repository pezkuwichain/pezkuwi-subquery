{
  "name": "subquery-pezkuwi",
  "version": "1.0.0",
  "description": "Pezkuwi SubQuery - Staking rewards, NominationPools, transfers indexer for PezWallet",
  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/subql build",
    "start:docker": "docker-compose pull && docker-compose up --remove-orphans",
    "codegen": "./node_modules/.bin/subql codegen",
    "validate": "./node_modules/.bin/subql validate",
    "local-publish": "cd ./scripts && ./local-publish.sh"
  },
  "files": [
    "dist",
    "schema.graphql",
    "pezkuwi.yaml",
    "pezkuwi-assethub.yaml"
  ],
  "author": "Pezkuwi Team",
  "license": "Apache-2.0",
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@polkadot/api": "^16",
    "@polkadot/api-derive": "^16",
    "@polkadot/types": "^16",
    "@polkadot/types-augment": "^16",
    "@polkadot/types-codec": "^16",
    "@subql/cli": "latest",
    "@subql/types": "latest",
    "@types/jest": "^29.5.1",
    "jest": "^29.5.0",
    "prettier": "3.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "ipfs-unixfs": "6.0.6"
  }
